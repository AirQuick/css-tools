        <xsl:variable name="state" as="item()+">
          <xsl:choose>
            <xsl:when test="$state[$p:l1] eq 6">                                    <!-- S -->
              <xsl:variable name="state" select="p:lookahead2(59, $input, $state)"/> <!-- S | IDENT | FUNCTION | NUMBER | HEX | PERCENTAGE | PLUS | COMMA |
