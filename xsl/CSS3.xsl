                </xsl:when>
                <xsl:when test="$state[$p:l1] = 4">                                 <!-- S -->
                  <xsl:variable name="state" select="p:consume(4, $input, $state)"/> <!-- S -->
                  <xsl:variable name="state" select="
                    if ($state[$p:error]) then
